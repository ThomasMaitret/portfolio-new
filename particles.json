{
  "fullScreen": {
    "enable": true,
    "zIndex": 1
  },
  "interactivity": {
    "events": {
      "onClick": {
        "mode": "push"
      },
      "onHover": {
        "mode": "repulse"
      }
    },
    "modes": {
      "bubble": {
        "distance": 400,
        "duration": 2,
        "opacity": 0.8,
        "size": 40
      },
      "grab": {
        "distance": 400
      }
    }
  },
  "particles": {
    "color": {
      "value": "#ffffff"
    },
    "links": {
      "color": {
        "value": "#ffffff"
      },
      "distance": 150,
      "opacity": 0.4
    },
    "move": {
      "attract": {
        "rotate": {
          "x": 600,
          "y": 1200
        }
      },
      "enable": true,
      "outModes": {
        "default": "destroy",
        "bottom": "destroy",
        "left": "destroy",
        "right": "destroy",
        "top": "destroy"
      },
      "speed": 5
    },
    "number": {
      "density": {
        "enable": true
      },
      "value": 0
    },
    "opacity": {
      "animation": {
        "speed": 3,
        "minimumValue": 0.1
      }
    },
    "size": {
      "value": {
        "min": 0.1,
        "max": 20
      },
      "animation": {
        "enable": true,
        "sync": true,
        "destroy": "max",
        "minimumValue": 0.1,
        "startValue": "min"
      }
    }
  },
  "emitters": {
    "autoPlay": true,
    "direction": "top",
    "life": {},
    "rate": {
      "quantity": 2,
      "delay": 0.1
    },
    "size": {
      "mode": "percent",
      "height": 0,
      "width": 100
    },
    "position": {
      "x": 50,
      "y": 100
    }
  }
}
